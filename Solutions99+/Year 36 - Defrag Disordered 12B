mem2 = foreachdir nw,w,sw,n,s,ne,e,se:
	mem3 = foreachdir nw,n,ne,e:
		mem1 = foreachdir nw,w,n,s,ne,e,se:
			step n
			if c == nothing:
				pickup s
				pickup w
				pickup se
				drop
			endif
		endfor
		mem2 = foreachdir nw,sw,n,s,ne,e,se:
			step s
		endfor
	endfor
	step e
endfor


